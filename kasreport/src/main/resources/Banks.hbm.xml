<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="com.dwilab.koprees.models.Banks" table="banks">
      
      <meta attribute = "class-description">
         This class contains the user detail. 
      </meta>
      
      <id name = "id" type = "int" column = "id">
         <generator class="native"/>
      </id>
      
      <property name = "createdAt" column = "created_at" type = "string"/>
      <property name = "updatedAt" column = "updated_at" type = "string"/>
      <property name = "deletedAt" column = "deleted_at" type = "string"/>
      <property name = "name" column = "name" type = "string"/>
      <property name = "debitChargeFormula" column = "debit_charge_formula" type = "string"/>
      <property name = "creditChargeFormula" column = "credit_charge_formula" type = "string"/>
   </class>
</hibernate-mapping>
